# Configuration Twilio
# Obtenez ces valeurs depuis votre console Twilio (https://console.twilio.com/)
TWILIO_ACCOUNT_SID=your_account_sid_here
TWILIO_AUTH_TOKEN=your_auth_token_here

# Clés API Twilio (optionnel - utilisez les identifiants de compte par défaut si non définis)
TWILIO_API_KEY=your_api_key_here
TWILIO_API_SECRET=your_api_secret_here

# Numéro de téléphone Twilio
# Le numéro que vous avez acheté sur Twilio
TWILIO_PHONE_NUMBER=+18199754345

# TwiML App SID
# Créez une TwiML App dans votre console Twilio et utilisez son SID
TWILIO_TWIML_APP_SID=AP771e654c2d516cb64d800ba30650ed9d

# Identité du client Twilio
# Nom d'utilisateur pour identifier le client dans les appels
TWILIO_CLIENT_IDENTITY=softphone-user

# Configuration du serveur
PORT=3000
NODE_ENV=development
NGROK_URL=https://your-ngrok-url.ngrok-free.app

# Configuration de sécurité (optionnel)
# Rate limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Configuration des logs
LOG_LEVEL=info 