{
  "name": "softphone-twilio",
  "version": "1.0.0",
  "description": "Softphone fonctionnel utilisant Node.js et Twilio SDK",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build:css": "tailwindcss -i ./public/tailwind.css -o ./public/tailwind-built.css",
    "build:css:watch": "tailwindcss -i ./public/tailwind.css -o ./public/tailwind-built.css --watch",
    "dev:full": "concurrently \"npm run build:css:watch\" \"npm run dev\"",
    "test": "node test-config.js",
    "test-api": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "softphone",
    "twilio",
    "voip",
    "nodejs"
  ],
  "author": "Simon",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "multer": "^2.0.2",
    "socket.io": "^4.7.4",
    "twilio": "^4.19.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0"
  }
}
